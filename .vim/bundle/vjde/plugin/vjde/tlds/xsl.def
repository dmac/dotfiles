tag apply-imports {
}
tag apply-templates  {
   attr select;
   attr mode;
}
tag attribute  {
   attr name;
   attr namespace;
   attr xml:space;
}
tag attribute-set  {
   attr name;
   attr use-attribute-sets;
}
tag call-template   {
   attr name;
}
tag choose {
   attr xml:space;
}

tag comment {
   attr xml:space;
}
tag copy  {
   attr use-attribute-sets;
   attr xml:space;
}
tag copy-of {
   attr select;
}
tag decimal-format  {
   attr NaN;
   attr name;
   attr decimal-separator;
   attr digit;
   attr grouping-separator;
   attr infinity;
   attr minus-sign;
   attr pattern-separator;
   attr per-mile;
   attr percent;
   attr zero-digit;
}
tag element  {
   attr name;
   attr use-attribute-sets;
   attr namespace;
   attr xml:space;
}
tag eval  {  
   attr language;
}

tag for-each  {
   attr select;
   attr xml:space;
}
tag fallback {
   attr xml:space;
}
tag if  {
   attr test;
   attr xml:space;
}
tag import  {
   attr href;
}
tag include  {
   attr href;
}
tag key  {
   attr match;
   attr name;
   attr use;
}
tag message {
   attr terminate {
      yes,
      no
   }
   attr xml:space;
}
tag namespace-alias  {
   attr result-prefix {
      #default
   }
   attr stylesheet-prefix {
      #default
   }
}
tag number  {
   attr count;
   attr format;
   attr from;
   attr grouping-separator;
   attr grouping-size;
   attr lang;
   attr letter-value;
   attr level {
      single,
      multiple,
      any
   }
   attr value;
}
tag otherwise {
   attr xml:space;
}
tag output  {
   attr method {
      xml,
      html,
      text
   }
   attr version {
      1.0
   }
   attr encoding {
      UTF-8,
      UTF-16,
      ISO-10646-UCS-2,
      ISO-10646-UCS-4,
      ISO-8859-
      ISO-8859-1,
      ISO-8859-2,
      ISO-8859-3,
      ISO-8859-4,
      ISO-8859-5,
      ISO-8859-6,
      ISO-8859-7,
      ISO-8859-8,
      ISO-8859-9,
      ISO-8859-10,
      ISO-2022-JP,
      Shift_JIS,
      EUC-JP,
      windows-1252
   }
   attr omit-xml-declaration {
      yes,
      no
   }
   attr standalone {
      yes,
      no
   }
   attr cdata-section-elements;
   attr indent {
      yes,
      no
   }
   attr doctype-public;
   attr doctype-system;
   attr media-type;
}
tag param {
   attr name;
   attr expr;
   attr select;
}
tag preserve-space  {
   attr elements;
}
tag processing-instruction {
   attr name;
   attr xml:space;
}
tag script  {  
   attr language;
}
tag sort  {
   attr lang;
   attr data-type;
   attr order {
      ascending,
      descending
   }
   attr case-order {
      upper-first,
      lower-first
   }
   attr select;
}
tag strip-space  {
   attr elements;
}
tag stylesheet  {
   attr version {
      1.0
   }
   attr xmlns:xsl {
      http://www.w3.org/1999/XSL/Transform
   }
   attr exclude-result-prefixes;
   attr extension-element-prefixes;
   attr id;
   attr xml:space;
}
tag template {
   attr match;
   attr name;
   attr priority;
   attr mode;
   attr xml:space;
}
tag text  {
   attr disable-output-escaping {
      yes,
      no
   }
}
tag transform {
   attr version {
      1.0,
   }
   attr xmlns:xsl {
      http://www.w3.org/1999/XSL/Transform
   }
   attr exclude-result-prefixes;
   attr extension-element-prefixes;
   attr id;
   attr xml:space;
}

tag value-of  {
   attr select;
   attr disable-output-escaping {
      yes,
      no
   }
}
tag variable  {
   attr name;
   attr expr;  
   attr select;
}
tag when  {
   attr test;
   attr xml:space;
}
tag with-param {
   attr name;
   attr select;
}
